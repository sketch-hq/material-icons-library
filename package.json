{
  "name": "sketch-synth",
  "version": "1.0.0",
  "description": "A proof of concept tool to generate Sketch files from other file formats",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -Rf foo.sketch dist",
    "build": "yarn clean && tsc --outDir dist src/index.ts",
    "start": "yarn build && node dist/index.js",
    "open": "yarn start && open -a Sketch foo.sketch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sketch-hq/sketch-synth.git"
  },
  "keywords": [
    "sketch"
  ],
  "author": "Ale M",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sketch-hq/sketch-synth/issues"
  },
  "homepage": "https://github.com/sketch-hq/sketch-synth#readme",
  "dependencies": {
    "adm-zip": "^0.5.1",
    "glob": "^7.1.6",
    "svgson": "^4.1.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@sketch-hq/sketch-file-format-ts": "^5.2.0",
    "@types/adm-zip": "^0.4.33",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.10",
    "@types/uuid": "^8.3.0",
    "svg-points": "^6.0.1",
    "typescript": "^4.1.2"
  }
}
